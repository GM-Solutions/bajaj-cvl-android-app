<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_near_by_labs"
    android:background="@drawable/splashbg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.loyaltyapp.co.example.loyaltyapp.activity.ProductLog"
    tools:showIn="@layout/activity_history">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/historysearchedittext"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="1dp"
        android:layout_alignParentTop="true"
        android:hint="Search By UPC Code or Part NO.."
        android:visibility="gone"
        android:padding="10dp"
        android:background="@drawable/square_box_blue"
        />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/historylistview"
        android:layout_below="@id/historysearchedittext"
        android:dividerHeight="5dp"
        android:padding="5dp"
        android:divider="@drawable/splashbg"/>

</RelativeLayout>